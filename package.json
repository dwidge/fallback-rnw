{
  "name": "@dwidge/fallback-rnw",
  "version": "0.0.6",
  "description": "Fallback component with context buttons for react-native-error-boundary.",
  "repository": "github:dwidge/fallback-rnw",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "rimraf dist && ncp assets/ dist/ && tsc -b . tsconfig.cjs.json",
    "test": "node --enable-source-maps --test --test-name-pattern"
  },
  "peerDependencies": {
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "react": "^19.0.0",
    "react-native": "^0.79.5",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "expect": "^29.7.0",
    "ncp": "^2.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=20"
  }
}
